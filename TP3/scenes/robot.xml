            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id = "leg"/>
            </children>
        </component>

        <component id="leg3">
            <transformation>
                <translate x="11" y="-10" z="-11" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id = "leg"/>
            </children>
        </component>

        <component id="leg4">
            <transformation>
                <translate x="11" y="-10" z="11" />
            </transformation>
            <materials>
