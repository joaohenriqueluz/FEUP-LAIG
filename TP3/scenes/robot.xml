<lxs>
    <scene root="root" axis_length="5"/>

    <views default="frontView">
        <perspective id="frontView" near="0.005" far="500" angle="45">
            <from x="0" y="65.5" z="90.5" />
            <to x="0" y="1.5" z="0" />
        </perspective>

        <ortho id="backView" near="0.005" far="500" left="-8.0" right="8.0" top="8.0" bottom="-8.0">
            <from x="11.13" y="7.65" z="-16.9" />
            <to x="1.96" y="3.35" z="0.18" />
            <up x="-0.11" y="1" z="0.19" />
        </ortho>

        <ortho id="otherView" near="0.005" far="500" left="-8.0" right="8.0" top="8.0" bottom="-8.0">
            <from x="5.13" y="10.65" z="-10.9" />
            <to x="5.96" y="2.35" z="1.18" />
            <up x="-0.11" y="1" z="0.19" />
        </ortho>
    </views>

    <globals>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>

    <lights>
        <!--<omni id="robotBackLight" enabled="0">
            <location x="-0.5" y="0.5" z="0" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.2" g="0.0" b="1.0" a="1.0" />
            <specular r="0.4" g="0.4" b="1.0" a="1.0" />
            <attenuation constant="0" linear="1" quadratic="0" />
        </omni>-->
        <spot id="robotFrontLight" enabled="1" angle="10" exponent="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <target x="0" y="0" z="0" />
            <ambient r="0.75" g="0.75" b="0.75" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="1" linear="0" quadratic="0" />
        </spot>
    </lights>

    <textures>
        <texture id="boardTexture" file="scenes/images/T1.png" />
        <texture id="wood" file="scenes/images/wood.jpg" />
        <texture id="roomLeft" file="scenes/images/LeftSpace.png" />
        <texture id="roomRight" file="scenes/images/rightSpace.png" />
        <texture id="roomTop" file="scenes/images/UpSpace.png" />
        <texture id="roomBottom" file="scenes/images/DownSpace.png" />
        <texture id="roomBack" file="scenes/images/BackSpace.png" />
        <texture id="roomFront" file="scenes/images/FrontSpace.png" />
        <texture id="moonSurfaceText" file="scenes/images/moonSurface.jpg" />
    </textures>

    <materials>
        <material id="whiteMaterial" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.9" g="0.9" b="0.9" a="1.0" />
            <diffuse r="0.9" g="0.9" b="0.9" a="1.0" />
            <specular r="0.9" g="0.9" b="0.9" a="1.0" />
        </material>

        <material id="yellowMaterial" shininess="5">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.97254" g="0.6667" b="0.0863" a="1.0" />
            <diffuse r="0.9" g="0.66" b="0.08" a="1.0" />
            <specular r="0.9" g="0.66" b="0.08" a="1.0" />
        </material>

        <material id="robotMaterialEyes" shininess="4">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.0" g="0.0" b="1.0" a="1.0" />
            <diffuse r="0.0" g="0.0" b="0.8" a="1.0" />
            <specular r="0.0" g="0.0" b="0.8" a="1.0" />
        </material>
    </materials>

    <transformations>
        <transformation id="robotInitialPos">
            <translate x="0" y="0" z="1.5" />
            <scale x="1.5" y="1.5" z="1.5" />
        </transformation>
    </transformations>

    <animations>
        <animation id="moveRobot">
            <keyframe instant="2">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="5">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="18.4" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="7">
                <translate x="5" y="-0.5" z="15" />
                <rotate angle_x="0" angle_y="18.4" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="13">
                <translate x="5" y="-0.5" z="15" />
                <rotate angle_x="0" angle_y="18.4" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="17">
                <translate x="5" y="-0.5" z="15" />
                <rotate angle_x="0" angle_y="-161.6" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="19">
                <translate x="2" y="0.0" z="1.5" />
                <rotate angle_x="0" angle_y="-161.6" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="22">
                <translate x="-1" y="-0.15" z="-12" />
                <rotate angle_x="0" angle_y="-161.6" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
        </animation>
    </animations>

    <primitives>
        <primitive id="rectangle">
            <rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />
        </primitive>
        <primitive id="triangle">
            <triangle x1="0.5" y1="0.0" z1="0.0" x2="0" y2="0.5625" z2="0.0" x3="-0.5" y3="0.0" z3="0.0" />
        </primitive>
        <primitive id="cylinder">
            <cylinder base="1" top="1" height="1" slices="10" stacks="10" />
        </primitive>
        <primitive id="sphere">
            <sphere radius="1" slices="20" stacks="20" />
        </primitive>
        <primitive id="cone">
            <cylinder base="1" top="0.001" height="1" slices="10" stacks="10" />
        </primitive>
        <primitive id="torus">
            <torus inner="0.15" outer="0.5" slices="20" loops="20" />
        </primitive>
        <primitive id="plane">
            <plane npartsU="2" npartsV="2" />
        </primitive>
        <primitive id="cylinderBase">
            <patch npointsU="2" npointsV="4" npartsU="10" npartsV="10">
                <controlpoint xx="-1" yy="0.0" zz="0" />
                <controlpoint xx="-1" yy="1.3333" zz="0" />
                <controlpoint xx="1" yy="1.3333" zz="0" />
                <controlpoint xx="1" yy="0.0" zz="0" />

                <controlpoint xx="-1" yy="0.0" zz="0" />
                <controlpoint xx="-1" yy="-1.3333" zz="0" />
                <controlpoint xx="1" yy="-1.3333" zz="0" />
                <controlpoint xx="1" yy="0.0" zz="0" /> 
            </patch>
        </primitive>
    </primitives>

    <components>
        <component id="root">
            <transformation>
            </transformation>
            <materials>
                <material id="whiteMaterial" />
            </materials>
            <texture id="none"/>
            <children>
                <componentref id="room" />
                <componentref id="table" />
                <componentref id="cylinderPiece" />
                <componentref id="cubePiece" />
                <componentref id="spherePiece" />
                <componentref id="conePiece" />
            </children>
        </component>

         <component id="table">
            <transformation>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="wood" length_s="1" length_t = "1"/>
            <children>
                <componentref id = "tablePrim"/>
                <componentref id = "leg1"/>
                <componentref id = "leg2"/>
                 <componentref id = "leg3"/>
                  <componentref id = "leg4"/>
            </children>
        </component>

        <component id="tablePrim">
            <transformation>
                <scale x="5" y="0.1" z="5"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id = "prism"/>
            </children>
        </component>

        <component id="leg1">
            <transformation>
                <translate x="-11" y="-10" z="11" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id = "leg"/>
            </children>
        </component>

        <component id="leg2">
            <transformation>
                <translate x="-11" y="-10" z="-11" />
