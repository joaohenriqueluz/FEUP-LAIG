<lxs>
    <scene root="scooter" axis_length="5"/>

    <views default="defaultCamera">
        <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="30" y="15" z="30" />
            <to x="0" y="-2" z="0" />
        </perspective>
    </views>

    <globals>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>

    <lights>
        <omni id="light" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="0" linear="1" quadratic="0" />
        </omni>
    </lights>

    <textures>
        <texture id="textureMetal" file="scenes/images/metal.jpg" />
        <texture id="textureRubber" file="scenes/images/rubber.jpg" />
    </textures>

    <materials>
        <material id="material" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.6" g="0.0" b="0.0" a="1.0" />
            <specular r="0.6" g="0.0" b="0.0" a="1.0" />
        </material>
    </materials>

    <transformations>
        <transformation id="transformation">
            <scale x="5" y="1" z="5" />
            <rotate axis="x" angle="-45" />
            <rotate axis="z" angle="-45" />
        </transformation>
    </transformations>

    <primitives>
        <primitive id="rectangle">
            <rectangle x1="-0.5" y1="-1" x2="0.5" y2="1" />
        </primitive>
        <primitive id="triangle">
            <triangle   x1="-0.5" y1="0" z1="0"
                        x2="0.5" y2="0" z2="0"
                        x3="0" y3="0.866" z3="0" />
        </primitive>
        <primitive id="cylinder">
            <cylinder base="0.5" top="0.5" height="1" slices="10" stacks="10" />
        </primitive>
        <primitive id="sphere">
            <sphere radius="1" slices="25" stacks="25" />
        </primitive>
        <primitive id="torus">
            <torus inner="0.35" outer="1" slices="25" loops="25" />
        </primitive>
    </primitives>

    <components>
        <component id="rectangleBottomRectCuboid">
            <transformation>
                <translate x="0" y="-0.5" z="0" />
                <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>
        <component id="rectangleTopRectCuboid">
            <transformation>
                <translate x="0" y="0.5" z="0" />
                <rotate axis="x" angle="90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>
        <component id="rectangleRightRectCuboid">
            <transformation>
                <translate x="0.5" y="0" z="0" />
                <scale x="1" y="0.25" z="2" />
                <rotate axis="y" angle="90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>
        <component id="rectangleLeftRectCuboid">
            <transformation>
                <translate x="-0.5" y="0" z="0" />
                <scale x="1" y="0.25" z="2" />
                <rotate axis="y" angle="-90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>
        <component id="rectangleFrontRectCuboid">
            <transformation>
                <translate x="0" y="0" z="1" />
                <scale x="1" y="0.25" z="1" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>
        <component id="rectangleBackRectCuboid">
            <transformation>
                <translate x="0" y="0" z="-1" />
                <scale x="1" y="0.25" z="1" />
                <rotate axis="y" angle="-180" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>
        <component id="boardMain">
            <transformation>
                <scale x="1" y="0.2" z="1.5" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <componentref id="rectangleBottomRectCuboid" />
                <componentref id="rectangleTopRectCuboid" />
                <componentref id="rectangleRightRectCuboid" />
                <componentref id="rectangleLeftRectCuboid" />
                <componentref id="rectangleFrontRectCuboid" />
                <componentref id="rectangleBackRectCuboid" />
            </children>
        </component>
        <component id="rectangleBackTriPrism">
            <transformation>
                <rotate axis="y" angle="-180" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>
        <component id="rectangleLeftTriPrism">
            <transformation>
                <translate x="-0.5" y="0" z="0" />
                <rotate axis="y" angle="120" />
                <translate x="-0.5" y="0" z="0" />
                <rotate axis="y" angle="180" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>
        <component id="rectangleRightTriPrism">
            <transformation>
                <translate x="0.5" y="0" z="0" />
                <rotate axis="y" angle="60" />
                <translate x="-0.5" y="0" z="0" />
            </transformation>
            <materials>
                <material id="material" length_s="1" length_t="1"/>
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>
        <component id="triangleBottomTriPrism">
            <transformation>
                <translate x="0" y="-1" z="0" />
                <rotate axis="x" angle="90" />
            </transformation>
            <materials>
                <material id="material" length_s="1" length_t="1"/>
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="triangle" />
            </children>
        </component>
        <component id="triangleTopTriPrism">
            <transformation>
                <translate x="0" y="1" z="0" />
                <rotate axis="y" angle="180" />
                <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="triangle" />
            </children>
        </component>
        <component id="boardBow">
            <transformation>
                <scale x="1" y="0.05" z="1" />
                <translate x="0" y="0" z="1.5" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <componentref id="rectangleBacktTriPrism" />
                <componentref id="rectangleLeftTriPrism" />
                <componentref id="rectangleRightTriPrism" />
                <componentref id="triangleBottomTriPrism" />
                <componentref id="triangleTopTriPrism" />
            </children>
        </component>
        <component id="board">
            <transformation>
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <componentref id="boardMain" />
                <componentref id="boardBow" />
            </children>
        </component>
        <component id="handleCentral">
            <transformation>
                <translate x="-1" y="4" z="0" />
                <scale x="2" y="0.225" z="0.225" />
                <rotate axis="y" angle="90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="cylinder" />
            </children>
        </component>
        <component id="handleRight">
            <transformation>
                <translate x="0.3" y="4" z="0" />
                <scale x="1" y="0.3" z="0.3" />
                <rotate axis="y" angle="90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="cylinder" />
            </children>
        </component>
        <component id="handleLeft">
            <transformation>
                <translate x="-1.3" y="4" z="0" />
                <scale x="1" y="0.3" z="0.3" />
                <rotate axis="y" angle="90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="cylinder" />
            </children>
        </component>
        <component id="handle">
            <transformation>
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <componentref id="handleCentral" />
                <componentref id="handleRight" />
                <componentref id="handleLeft" />
            </children>
        </component>
        <component id="bell">
            <transformation>
                <translate x="0.5" y="4.1" z="-2" />
                <scale x="0.15" y="0.15" z="0.15" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="sphere" />
            </children>
        </component>
        <component id="handleSet">
            <transformation>
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <componentref id="bell" />
                <componentref id="handle" />
            </children>
        </component>
        <component id="connectiveFrame">
            <transformation>
                <translate x="0" y="1" z="0" />
                <rotate axis="x" angle="45" />
                <scale x="0.225" y="0.225" z="1.45" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="cylinder" />
            </children>
        </component>
        <component id="innerCentralFrame">
            <transformation>
                <scale x="0.225" y="4" z="0.225" />
                <translate x="0" y="1" z="0" />
                <rotate axis="x" angle="90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="cylinder" />
            </children>
        </component>
        <component id="midCentralFrame">
            <transformation>
                <scale x="0.275" y="2.5" z="0.275" />
                <translate x="0" y="1" z="0" />
                <rotate axis="x" angle="90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="cylinder" />
            </children>
        </component>
        <component id="outerCentralFrame">
            <transformation>
                <scale x="0.35" y="0.7" z="0.35" />
                <translate x="0" y="1" z="0" />
                <rotate axis="x" angle="90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="cylinder" />
            </children>
        </component>
        <component id="frontSet">
            <transformation>
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <componentref id="innerCentralFrame" />
                <componentref id="midCentralFrame" />
                <componentref id="outerCentralFrame" />
            </children>
        </component>
        <component id="bodySet">
            <transformation>
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <componentref id="frontSet" />
                <componentref id="connectiveFrame" />
                <componentref id="handleSet" />
            </children>
        </component>
        <component id="wheelInside">
            <transformation>
                <scale x="0.1" y="0.2" z="0.2" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="sphere" />
            </children>
        </component>
        <component id="wheelOutside">
            <transformation>
                <scale x="0.35" y="0.35" z="0.35" />
                <rotate axis="y" angle="90" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="textureRubber" />
            <children>
                <primitiveref id="torus" />
            </children>
        </component>
        <component id="frontWheel">
            <transformation>
                <translate x="0" y="-0.1" z="-2" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <componentref id="wheelInside" />
                <componentref id="wheelOutside" />
            </children>
        </component>
        <component id="backWheel">
            <transformation>
                <translate x="0" y="-0.1" z="1.5" />
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <componentref id="wheelInside" />
                <componentref id="wheelOutside" />
            </children>
        </component>
        <component id="wheelSet">
            <transformation>
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="inherit" />
            <children>
                <componentref id="frontWheel" />
                <componentref id="backWheel" />
            </children>
        </component>
        <component id="scooter">
            <transformation>
            </transformation>
            <materials>
                <material id="material" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="bodySet" />
                <componentref id="wheelSet" />
            </children>
        </component>
    </components>
</lxs>